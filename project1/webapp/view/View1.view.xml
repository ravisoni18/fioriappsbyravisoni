   <mvc:View
	controllerName="project1.controller.View1"
	xmlns="sap.m"
	 xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
	 xmlns:viz="sap.viz.ui5.controls"
	  xmlns:layout="sap.ui.layout"
		xmlns:smc="sap.ui.comp.smartmicrochart"
	xmlns:mvc="sap.ui.core.mvc"
			xmlns:mc="sap.suite.ui.microchart"
 xmlns:viz.data="sap.viz.ui5.data"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
	xmlns:core="sap.ui.core"
    xmlns:forms="sap.ui.layout.form"
	displayBlock="true"
	height="100%">
	<Page>
	<f:Card class="sapUiMediumMargin" width="300px" >
		<f:header>
			<card:Header 
                press=".onClickCard"
				title="Sales Order Data"
				subtitle="Enter Sales Order Fi"
				iconSrc="sap-icon://chart" />
		</f:header>
		<f:content >
			<VBox
				>
                <forms:SimpleForm editable="true" layout="ColumnLayout">

		<core:Title text="General"/>

		<Label text="Enter Duration" />
	<DateRangeSelection
				width="100%"
                displayFormat="dd.MM.yyyy"
				valueState="{valueState}"
				valueStateText="{valueStateText}"/>
		<Label text="Select Customer"/>
		<Input
			id="productInput"
			textFormatMode="ValueKey"
			placeholder="Enter Customer"
			showSuggestion="true"
			showTableSuggestionValueHelp="false"
			suggestionRows="{/ProductCollection}"
			suggestionItemSelected=".onSuggestionItemSelected">
			<suggestionColumns>
				<Column
					popinDisplay="Inline"
					demandPopin="true">
					<Label text="Name" />
				</Column>
				<Column
					hAlign="Center"
					popinDisplay="Inline"
					demandPopin="true"
					minScreenWidth="Tablet">
					<Label text="Product ID" />
				</Column>
				<Column
					hAlign="Center"
					popinDisplay="Inline"
					minScreenWidth="Tablet">
					<Label text="Supplier Name" />
				</Column>
				<Column
					hAlign="End"
					popinDisplay="Inline"
					demandPopin="true">
					<Label text="Price" />
				</Column>
			</suggestionColumns>

			<suggestionRows>
				<ColumnListItem>
					<Label text="{Name}" />
					<Label text="{ProductId}" />
					<Label text="{SupplierName}" />
					<Label text="{
						parts:[{path:'Price'}, {path:'CurrencyCode'}],
						type: 'sap.ui.model.type.Currency',
						formatOptions: {showMeasure: true}
					}" />
				</ColumnListItem>
			</suggestionRows>
		</Input>

		<Label text="Job title"/>
		<Input value="Developer"/>

	</forms:SimpleForm>

					
				<Button text="Default"
						press="onPress"
						ariaDescribedBy="defaultButtonDescription genericButtonDescription">
					<layoutData>
						<FlexItemData growFactor="3"  />
					</layoutData>
				</Button>
				

			</VBox>
		</f:content>
	</f:Card>

	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="Project Cloud Transformation" subtitle="Revenue per Product | EUR" />
		</f:header>
		<f:content>
			<List
				showSeparators="None"
				items="{
					path: 'DataModel>/ProductCollection'
				}" growing="true"
		growingThreshold="4"
		growingScrollToLoad="false"  >
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{DataModel>Name}" />
							<Text text="{DataModel>SupplierName}" />
						</VBox>
						<ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{DataModel>ProductId}"
							 />
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>
	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="Project Cloud Transformation" subtitle="Revenue per Product | EUR" />
		</f:header>
		<f:content>
				<Panel headerText="{/description}"
				 class="sapUiSmallMarginBottom">
				<mc:LineMicroChart
						color="{color}"
						showPoints="{showPoints}"
						size="L"
						threshold="{/threshold}"
						showThresholdLine="{/showThresholdLine}"
						leftTopLabel="{/leftTopLabel}"
						leftBottomLabel="{/leftBottomLabel}"
						rightTopLabel="{/rightTopLabel}"
						rightBottomLabel="{/rightBottomLabel}"
						showTopLabels="{showTopLabels}"
						showBottomLabels="{showBottomLabels}"
						press="press"
						class="sapUiTinyMarginTop"
						lines="{
							path: '/lines',
							templateShareable: true
						}">
					<mc:lines>
						<mc:LineMicroChartLine
								points="{
								path: 'points',
								templateShareable: true
							}"
								color="{color}"
								showPoints="{showPoints}"
								type="{type}"
								>
							<mc:LineMicroChartPoint x="{x}" y="{y}"/>
						</mc:LineMicroChartLine>
					</mc:lines>
				</mc:LineMicroChart>
		</Panel>
		</f:content>
	</f:Card>


	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="Sales by Customers Chart" subtitle="Sales Revenue by Customers | EUR" />
		</f:header>
		<f:content>
				<Panel headerText="{/description}"
				 class="sapUiSmallMarginBottom" height="500px" width="500px">
				 <!-- <layout:flexContent> -->
            <viz:Popover id="idPopOver"></viz:Popover>
            <viz:VizFrame id="idVizFrame" 
                height='100%' width="100%" vizType='pie' >
                <viz:dataset>
                    <viz.data:FlattenedDataset data="{/milk}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition name="Customer"
                                value="{Store Name}" />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition name="Revenue"
                                value="{Revenue}" />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem uid="size" type="Measure"
                        values="Revenue" />
                    <viz.feeds:FeedItem uid="color" type="Dimension"
                        values="Customer" />
                </viz:feeds>
            </viz:VizFrame>
        <!-- </layout:flexContent> -->
		</Panel>
		</f:content>
	</f:Card>
	</Page>
</mvc:View>
